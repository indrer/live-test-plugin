{
    "name": "Live test plugin",
    "version": "1.0",
    "manifest_version": 2,
    "background" : {
      "scripts": ["js/background.js"]
    },
    "browser_action": {
      "default_popup": "html/popup.html"
    },
    "permissions": ["windows", "tabs"],
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js":["js/content.js"]
      }
    ],
    "permissions": ["downloads"]
  }